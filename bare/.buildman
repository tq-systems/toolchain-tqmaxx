arm = /opt/OSELAS.Toolchain-2020.08.0/arm-v7a-linux-gnueabihf/gcc-10.2.1-clang-10.0.1-glibc-2.32-binutils-2.35-kernel-5.8-sanitized
powerpc = /opt/OSELAS.Toolchain-2020.08.0/powerpc-603e-linux-gnu/gcc-10.2.1-glibc-2.32-binutils-2.35-kernel-5.8-sanitized

[toolchain-prefix]
# name = path to prefix
aarch64 = /opt/OSELAS.Toolchain-2020.08.0/aarch64-v8a-linux-gnu/gcc-10.2.1-clang-10.0.1-glibc-2.32-binutils-2.35-kernel-5.8-sanitized/bin/aarch64-v8a-linux-gnu-
arm = /opt/OSELAS.Toolchain-2020.08.0/arm-v7a-linux-gnueabihf/gcc-10.2.1-clang-10.0.1-glibc-2.32-binutils-2.35-kernel-5.8-sanitized/bin/arm-v7a-linux-gnueabihf-
